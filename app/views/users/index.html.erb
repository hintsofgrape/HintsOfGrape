<h1>Listing users</h1>

<table>
<% for user in @users %>
  <tr>
    <td><%=h user.name %></td>
    <td>
      <%= link_to 'Show', user %><br/>
      <%= link_to 'Edit', edit_user_path(user) %><br/>
      <%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %><br/>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>
